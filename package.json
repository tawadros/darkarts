{
	"name": "darkarts",
	"version": "0.2.0",
	"repository": "https://github.com/tawadros/darkarts.git",
	"author": "tawadros <104744090+tawadros@users.noreply.github.com>",
	"license": "GPL-3.0-only",
	"engines": {
		"node": ">=10.4.0"
	},
	"scripts": {
		"format:project": "prettier '**/*.{ts,js,json,md}' --write",
		"format:contracts": "prettier 'contracts/**/*.sol' --write",
		"format:frontend": "prettier 'src/**/*.tsx' --write",
		"format": "npm run format:project && npm run format:contracts && npm run format:frontend",
		"lint:project": "eslint '**/*.{ts,js}'",
		"lint:contracts": "solhint 'contracts/**/*.sol'",
		"lint:frontend": "next lint",
		"lint": "npm run lint:project && npm run lint:contracts && npm run lint:frontend",
		"build:circuits": "mkdir -p build/circuits/{1to1mimc,4to1mimc,send,withdraw} && node scripts/compileZoKrates.js",
		"build:contracts": "hardhat compile",
		"build:frontend": "next build",
		"build": "npm run build:circuits && npm run build:contracts && npm run build:frontend",
		"test:contracts": "hardhat test",
		"test": "npm run test:contracts",
		"deploy:contracts:local": "hardhat run scripts/deploy.js --network localhost",
		"serve:dev": "next dev",
		"serve:build": "next start"
	},
	"dependencies": {
		"@mantine/core": "^4.2.6",
		"@mantine/hooks": "^4.2.6",
		"ethers": "^5.5.4",
		"fixed-merkle-tree": "^0.6.0",
		"next": "12.1.6",
		"react": "18.1.0",
		"react-dom": "18.1.0",
		"wagmi": "^0.4.3",
		"zokrates-js": "1.0.36"
	},
	"devDependencies": {
		"@nomiclabs/hardhat-ethers": "^2.0.5",
		"@nomiclabs/hardhat-etherscan": "^3.0.3",
		"@nomiclabs/hardhat-waffle": "^2.0.3",
		"@openzeppelin/contracts": "^4.3.3",
		"@types/node": "17.0.36",
		"@types/react": "18.0.9",
		"@types/react-dom": "18.0.5",
		"chai": "^4.3.6",
		"circomlibjs": "^0.1.2",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-next": "12.1.6",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-promise": "^5.2.0",
		"ethereum-waffle": "^3.4.0",
		"hardhat": "^2.9.1",
		"hardhat-gas-reporter": "^1.0.8",
		"prettier": "^2.5.1",
		"prettier-plugin-solidity": "^1.0.0-beta.13",
		"solc": "^0.8.10",
		"solhint": "^3.3.7",
		"solidity-coverage": "^0.7.20",
		"typescript": "4.7.2"
	}
}
